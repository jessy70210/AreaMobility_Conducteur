                android:paddingTop="@dimen/nav_header_vertical_spacing"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/image_apple_car"
                app:srcCompat="@drawable/ic_waze" />

            <TextView
                android:id="@+id/tv_waze"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lib_waze"
                app:layout_constraintBottom_toBottomOf="@+id/image_waze"
                app:layout_constraintEnd_toStartOf="@+id/s_waze"
                app:layout_constraintStart_toEndOf="@+id/image_waze"
                app:layout_constraintTop_toTopOf="@+id/image_waze" />

            <Switch
                android:id="@+id/s_waze"
