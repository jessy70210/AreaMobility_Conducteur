                app:srcCompat="@drawable/ic_sygic" />

            <TextView
                android:id="@+id/tv_sygic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lib_sygic"
                app:layout_constraintBottom_toBottomOf="@+id/image_sygic"
                app:layout_constraintEnd_toStartOf="@+id/s_sygic"
                app:layout_constraintStart_toEndOf="@+id/image_sygic"
                app:layout_constraintTop_toTopOf="@+id/image_sygic" />

            <Switch
                android:id="@+id/s_sygic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
