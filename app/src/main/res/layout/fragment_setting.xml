            <ImageView
                android:id="@+id/image_google_maps"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="16dp"
                android:paddingTop="@dimen/nav_header_vertical_spacing"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_andoid_auto" />

            <TextView
                android:id="@+id/tv_google_maps"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lib_android_auto_maps"
                app:layout_constraintBottom_toBottomOf="@+id/image_google_maps"
                app:layout_constraintEnd_toStartOf="@+id/s_google_maps"
                app:layout_constraintStart_toEndOf="@+id/image_google_maps"
                app:layout_constraintTop_toTopOf="@+id/image_google_maps" />

            <Switch
                android:id="@+id/s_google_maps"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/image_google_maps"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/image_google_maps"
                app:layout_constraintVertical_bias="0.518" />

            <ImageView
                android:id="@+id/image_apple_car"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="16dp"
                android:paddingTop="@dimen/nav_header_vertical_spacing"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/image_google_maps"
                app:srcCompat="@drawable/ic_car_play" />

            <TextView
                android:id="@+id/tv_apple_car"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lib_apple_car_plan"
                app:layout_constraintBottom_toBottomOf="@+id/image_apple_car"
                app:layout_constraintEnd_toStartOf="@+id/s_apple_car"
                app:layout_constraintStart_toEndOf="@+id/image_apple_car"
                app:layout_constraintTop_toTopOf="@+id/image_apple_car" />

            <Switch
                android:id="@+id/s_apple_car"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/image_apple_car"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/image_apple_car"
                app:layout_constraintVertical_bias="0.518" />

            <ImageView
                android:id="@+id/image_waze"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginStart="28dp"
